	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/reTHINK-project/dev-runtime-nodejs.git"
	},
	"scripts": {
		"start": "mkdir -p dist/ && babel --presets es2015 src/ --out-dir dist/  && babel --presets es2015 server.js -o dist/server.js && node dist/server.js",
		"transpile-dep": "babel --presets es2015 ./node_modules/service-framework/src/ --out-dir dist/service-framework/ && babel --presets es2015 ./node_modules/runtime-core/ --out-dir dist/runtime-core/",
		"test": "karma start",
		"init-setup": "npm install -g jspm karma-cli gulp-cli && npm install && jspm install"
	},
	"dependencies": {
		"array.observe": "0.0.1",
